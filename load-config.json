{
    "services": [
        {
            "name": "GoKit",
            "start": "go run ~/go/src/github.com/go-kit/kit/examples/stringsvc1/main.go",
            "req": "POST http://localhost:8080/uppercase\n@./gokit-req.json"
        },
        {
            "name": "Rocket",
            "start": "cargo run --manifest-path hello-rocket/Cargo.toml",
            "req": "GET http://localhost:8000"
        },
        {
            "name": "Node",
            "start": "node node.js",
            "req": "GET http://localhost:3301"
        },
        {
            "name": "Spring-Boot-Tomcat",
            "start": "java -jar spring-boot-tomcat/target/gs-spring-boot-0.1.0.jar --server.port=3302",
            "req": "GET http://localhost:3302"
        },
        {
            "name": "Spring-Boot-Jetty",
            "start": "java -jar spring-boot-jetty/target/gs-spring-boot-0.1.0.jar --server.port=3303",
            "req": "GET http://localhost:3303"
        },
        {
            "name": "Spring-Boot-Undertow",
            "start": "java -jar spring-boot/spring-boot-samples/spring-boot-sample-undertow/target/spring-boot-sample-undertow-2.0.0.BUILD-SNAPSHOT.jar --server.port=3304",
            "req": "GET http://localhost:3304"
        },
        {
            "name": "Green-Lightning",
            "start": "java -jar green-lightning/target/gl-hello.jar",
            "req": "GET http://localhost:3305/hello"
        },
        {
            "name": "Squall",
            "start": "java -jar squall/target/squall-hello.jar",
            "req": "GET http://localhost:3306/hello"
        }
    ]
}
