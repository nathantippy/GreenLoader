{
    "services": [
        {
            "name": "Node",
            "start": "node node.js",
            "endpoint": "http://127.0.0.1:3301",
            "method": "POST",
            "payload": '{"name": "Bench"}'
        },
        {
            "name": "Spring-Boot-Tomcat",
            "start": "java -jar spring-boot-tomcat/target/gs-spring-boot-0.1.0.jar --server.port=3302",
            "endpoint": "http://127.0.0.1:3302",
            "method": "POST",
            "payload": '{"name": "Bench"}'
        },
        {
            "name": "Spring-Boot-Jetty",
            "start": "java -jar spring-boot-jetty/target/gs-spring-boot-0.1.0.jar --server.port=3303",
            "endpoint": "http://127.0.0.1:3303",
            "method": "POST",
            "payload": '{"name": "Bench"}'
        },
        {
            "name": "Green-Lightning-Telemetry",
            "start": "java -jar green-lightning/target/gl-hello.jar true 3305",
            "endpoint": "http://127.0.0.1:3305/hello",
            "method": "POST",
            "payload": '{"name": "Bench"}'
        },
        {
            "name": "Green-Lightning-No-Telemetry",
            "start": "java -jar green-lightning/target/gl-hello.jar false 3307",
            "endpoint": "http://127.0.0.1:3307/hello",
            "method": "POST",
            "payload": '{"name": "Bench"}'
        },
        {
            "name": "Squall",
            "start": "java -jar squall/target/squall-hello.jar",
            "endpoint": "http://127.0.0.1:3306/hello",
            "method": "POST",
            "payload": '{"name": "Bench"}'
        }
    ]
}
